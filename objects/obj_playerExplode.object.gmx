        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Create the particle systems

// Create an absolute particle system (pos 0,0)
system = part_system_create();
part_system_depth(system ,210);

// Fire 
fire = part_type_create();
part_type_sprite(fire,spr_fireParticles,false,true,true);
part_type_alpha3(fire,1,0.7,0);
//part_type_size(fire,0.05,0.5,0.01,0);
part_type_size(fire,0.05,0.8,0.02,0);
part_type_color3(fire,make_color_rgb(255,232,117),make_color_rgb(255,96,0),make_color_rgb(127,0,0));
part_type_speed(fire,1,1,0,0);
part_type_gravity(fire,0.4,90);
part_type_direction(fire,0,360,0,0);
part_type_blend(fire,1);
part_type_life(fire,15,40);
part_type_orientation(fire,0,359,1,1,1);
fireEmitter = part_emitter_create(system);

ash = part_type_create();
part_type_shape(ash,pt_shape_pixel);
part_type_alpha2(ash,1,0);
part_type_size(ash,1,1,0,0);
part_type_color3(ash,make_color_rgb(255,0,0),make_color_rgb(0,0,255),make_color_rgb(255,0,0));
part_type_speed(ash,5,10,0,0);
part_type_direction(ash,80,100,0,0);
part_type_blend(ash,0);
part_type_life(ash,15,30);

fireSmokeSystem = part_system_create();
part_system_depth(fireSmokeSystem, 220); // behind the base system
fireSmoke = part_type_create();
part_type_shape(fireSmoke, pt_shape_cloud);
part_type_size(fireSmoke,0.60,1.2,0.05,0);
part_type_color3(fireSmoke,c_black,c_dkgray,c_gray);
part_type_alpha2(fireSmoke,1,0);
part_type_speed(fireSmoke,2,3,0,0);
part_type_gravity(fireSmoke,0.4,90);
part_type_direction(fireSmoke,80,100,0,0);
part_type_orientation(fireSmoke,0,0,0,0,1);
part_type_blend(fireSmoke,0);
part_type_life(fireSmoke,35,50);

fireSmokeEmitter = part_emitter_create(fireSmokeSystem);

</string>
